{
  "name": "root/test",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "AppBundle\\": "src/AppBundle",
      "Mvc\\": "src/AppBundle/Mvc",
      "Mvc\\Shared\\": "src/AppBundle/Shared"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  },
  "require": {
    "php": ">=7.1.26",
    "ext-curl": "*",
    "ext-ftp": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mongodb": "*",
    "ext-pdo": "*",
    "alcaeus/mongo-php-adapter": "^1.1",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "doctrine/mongodb": "1.6.*",
    "doctrine/mongodb-odm": "^1.2",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "drahosistvan/vatvalidation": "^1.0",
    "friendsofsymfony/jsrouting-bundle": "^2.1",
    "friendsofsymfony/rest-bundle": "^2.3",
    "friendsofsymfony/user-bundle": "^2.1",
    "incenteev/composer-parameter-handler": "^2.1",
    "jms/serializer-bundle": "^2.3",
    "knplabs/knp-snappy-bundle": "^1.5",
    "mongodb/mongodb": "^1.3",
    "nelmio/cors-bundle": "^1.5",
    "ramsey/uuid": "^3.9",
    "ramsey/uuid-doctrine": "^1.6",
    "rbdwllr/reallysimplejwt": "^2.1",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "symfony/http-client": "^4.3",
    "symfony/monolog-bundle": "^3.2",
    "symfony/polyfill-apcu": "^1.7",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/symfony": "^3.4",
    "symfony/twig-bundle": "^3.4",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "behat/symfony2-extension": "^2.1",
    "fzaninotto/faker": "^1.9",
    "phpspec/prophecy": "^1.9",
    "phpstan/phpstan-symfony": "^0.12.6",
    "sensio/generator-bundle": "^3.0",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/phpunit-bridge": "^4.3",
    "webmozart/assert": "^1.8"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ],
    "cli": "app/console --ansi -vv",
    "lint": "vendor/bin/phpcs",
    "lint:fix": "vendor/bin/phpcbf",
    "lint-security": "php -d memory_limit=-1 vendor/bin/phpstan analyse -c phpstan.neon"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": null
  },
  "provide": {
    "ext-mongo": "1.6.14"
  }
}
