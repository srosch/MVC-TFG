{
  "name": "drahosistvan/vatvalidation",
  "type": "library",
  "description": "A simple wrapper for VIES VAT Validation WebService.",
  "keywords": [
    "vies",
    "vat",
    "validation",
    "eu vat",
    "vat validation"
  ],
  "homepage": "https://github.com/drahosistvan/vatvalidation",
  "support": {
    "issues": "https://github.com/drahosistvan/vatvalidation/issues",
    "source": "https://github.com/drahosistvan/vatvalidation"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Istvan Drahos",
      "email": "hello@isti.hu",
      "homepage": "http://isti.hu"
    }
  ],
  "require": {
    "php": ">=5.0.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "VatValidation\\": "src/VatValidation/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit; ./vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
    "phpunit": "./vendor/bin/phpunit;",
    "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
  }
}
